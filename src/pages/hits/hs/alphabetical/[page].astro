---
import HitsPage from "@components/hits/HitsPage.astro";
import { getHitsounds } from "@ssg/hits";

export async function getStaticPaths({ paginate }) {
  let pages = paginate(await getHitsounds(true), {
    pageSize: 12,
  });
  return pages;
}

const { page } = Astro.props;
---

<HitsPage page={page} hitsounds={true} alpha={true} />
