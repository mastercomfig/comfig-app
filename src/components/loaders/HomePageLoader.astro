<home-page-loader></home-page-loader>

<script>
  import { homePage } from "@utils/index.ts";

  class HomePageLoader extends HTMLElement {
    async connectedCallback() {
      await homePage();
    }
  }

  if (!customElements.get("home-page-loader")) {
    customElements.define("home-page-loader", HomePageLoader);
  }
</script>
